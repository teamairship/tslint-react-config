{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier",
    "@airship/tslint-react-a11y"
  ],
  "linterOptions": { "exclude": ["node_modules/**/*.ts"] },
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "member-access": [true, "no-public"],
    "ordered-imports": false
  }
}
